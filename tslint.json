{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "no-console": false,
    "import-spacing": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-bitwise": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
